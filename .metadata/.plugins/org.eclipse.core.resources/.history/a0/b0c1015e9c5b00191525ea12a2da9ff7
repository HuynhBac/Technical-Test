package Question1;

public class Characteristic {

	public final int Wheel = 4;  // set final value of wheel, doors and set
	public final int doors = 4;
	public final int seats = 5;
	public int Maxspeed;

	public int getMaxspeed() {   //GETTER
		return Maxspeed;
	}

	public void setMaxspeed(int maxspeed) { //SETTER
		Maxspeed = maxspeed;
	}

	public void Info() {
		System.out.println("wheel = " + Wheel);
		System.out.println("doors = " + doors);
		System.out.println("Seats = " + seats);
		System.out.println("maxSpeed = " + this.Maxspeed);
		System.out.println("------------------------");
	}

}
