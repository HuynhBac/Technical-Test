package Queation2;

public class ToyotaCar implements Runnable {

	public void run() 
	{
			int wheel = 4;
			System.out.println("wheel = " + wheel);
			int doors = 4;
			System.out.println("Doors = " + doors);
			int seats = 5;
			System.out.println("Seats = " + seats);
			int maxSpeed = 100;
			for (int i=0;i<10;i++)
				{
					System.out.println((i+1)+".maxSpeed = " + maxSpeed);
				}
			
	}

