package Queation2;

public class BMWCar implements Characteristic{
	@Override
	public void wheel() {
		int wheel = 4;
		System.out.println("Wheel = " + wheel);
		
	}

	@Override
	public void doors() {
		int doors = 4;
		System.out.println("Doors = " + doors);
		
	}

	@Override
	public void seats() {
		int seats = 5;
		System.out.println("Seats = " + seats);
		
	}


	@Override
	public void maxSpeed() {
		int maxSpeed = 200;
		for (int i=0;i<10;i++)
		{
			System.out.println((i+1)+".maxSpeed = " + maxSpeed);
		}
		
	}

}
