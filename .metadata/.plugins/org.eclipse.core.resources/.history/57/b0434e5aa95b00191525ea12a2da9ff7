package Question1;

public class BMWCar extends Characteristic {
	public void run() {
		
		final int Maxspeed1 = 200;
		System.out.println("BMW Car Characteristic");
		super.Info();
		for (int j = 0; j < 10; j++) {
			System.out.println((j+1) +".Maxspeed = " + Maxspeed1);
		}
		System.out.println("--------------------------");
	}
}
