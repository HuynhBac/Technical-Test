package Queation2;

public class ToyotaCar implements Characteristic, Runnable {

	@Override
	public void wheel() {
		int wheel = 4;

		
	}

	@Override
	public void doors() {
		int doors = 4;
		System.out.println("Doors = " + doors);
		
	}

	@Override
	public void seats() {
		int seats = 5;
		System.out.println("Seats = " + seats);
		
	}


	@Override
	public void maxSpeed() {
		int maxSpeed = 100;
		for (int i=0;i<10;i++)
		{
			System.out.println((i+1)+".maxSpeed = " + maxSpeed);
		}

	}

	@Override
	public void run() {
System.out.println("dsfsdf");
	}
	} 
	
	
