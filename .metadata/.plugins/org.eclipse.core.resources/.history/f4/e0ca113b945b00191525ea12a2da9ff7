package Q1;

public class Characteristic {
	
	public final int wheel = 4;
	public final int doors = 4;
	public final int seats = 5;
	
	public int getWheel() {
		return wheel;
	}
	public void setWheel(int wheel) {
		this.wheel = wheel;
	}
	public int getDoors() {
		return doors;
	}
	public void setDoors(int doors) {
		this.doors = doors;
	}
	public int getSeat() {
		return seats;
	}
	public void setSeat(int seats) {
		this.seats = seats;
	}
	
	public void Info()
	{
		System.out.println("wheel = "+ wheel);
		System.out.println("doors = "+ doors);
		System.out.println("seat = "+ seats);
	}
}
